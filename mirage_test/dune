(executable
  (name test_mirage)
  (libraries mirage-block-unix mirage-clock-unix mirage-kv logs.cli logs.fmt lwt chamelon.kv fpath alcotest alcotest-lwt)
)

(rule
  (alias runtest)
  (action (progn
            (run dd if=/dev/zero of=emptyfile bs=4K count=10)
            (run %{exe:test_mirage.exe} -e)
            )))
