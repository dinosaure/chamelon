(executable
  (name test_mirage)
  (libraries mirage-block-unix mirage-clock-unix mirage-kv logs.cli logs.fmt lwt littlefs.kv fpath alcotest alcotest-lwt)
)

(rule
  (alias runtest)
  (action (progn
            (run dd if=/dev/zero of=emptyfile bs=1M count=1)
            (run %{exe:test_mirage.exe})
            )))
