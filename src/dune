(executables
  (public_names format)
  (libraries cmdliner littlefs.fs mirage-block-unix lwt)
)

(rule
  (alias runtest)
  (action (progn
            (run dd if=/dev/zero of=test.img bs=64K count=1)
            (run %{exe:format.exe} test.img)
            (run grep littlefs test.img)
            (run readmdir.py -a --log test.img 4096 0)
            (run readtree.py -a --log test.img 4096 0)
            ))
  )
